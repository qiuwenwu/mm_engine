{
	"restartable": "rs",
	"ignore": [
		".git",
		".svn",
		"cache",
		"log",
		"node_modules/**/node_modules",
		"game/cache",
		"game/data",
		"*/static/*/*.json",
		"*/static/*.js"
	],
	"verbose": true,
	"execMap": {
		"": "node",
		"js": "node --harmony"
	},
	"watch": [
		"app",
		"game",
		"lib",
		"core",
		"test"
	],
	"env": {
		"NODE_ENV": "development"
	},
	"ext": "js ts json",
	"legacy-watch": false
}
